    "version": "0.2.0",
    "configurations": [
        {
            "name": "FastAPI Remote Debugger",
            "type": "debugpy",
            "request": "attach",
            "connect": {
                "port": 5678,
                "host": "localhost"
            }
        },
        {
            "name": "frontend debug",
            "type": "msedge",
            "request": "launch",
            "url": "http://localhost:5173",
            "webRoot": "${workspaceFolder}/ui"
        }
    ]
}